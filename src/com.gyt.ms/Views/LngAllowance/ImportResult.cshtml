                                            <th>批次</th>
                                            <th>车牌号</th>
                                            <th>发动机号</th>
                                            <th>气瓶1编号</th>
                                            <th>气瓶2编号</th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        @foreach (var item in existedList)
                                        {
                                            <tr>
                                                <td class="center">
                                                    <label>
                                                        <input type="checkbox" class="ace" />
                                                        <span class="lbl"></span>
                                                    </label>
                                                </td>
                                                <td>
                                                    <a href="javascript:;">@item.Id</a>
                                                </td>
                                                <td>
                                                    @item.CompanyName
                                                </td>
                                                <td>
                                                    @item.LotId
                                                </td>
                                                <td>
                                                    @item.TruckNo
                                                </td>
                                                <td>@item.EngineId</td>
                                                <td>
                                                    @item.CylinderDefaultId.Replace('_', '-')
                                                </td>
                                                <td>
                                                    @(item.CylinderSeconedId.IsNullOrEmpty() ? "无" : item.CylinderSeconedId.Replace('_', '-'))
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>



                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

<!--导入成功的列表-->

@if (successList.IsNullOrEmpty())
{
    <div class="alert alert-block alert-danger">
        <button type="button" class="close" data-dismiss="alert">
            <i class="icon-remove"></i>
        </button>

        <i class="icon-exclamation-sign white"></i>

        <strong class="green">
          所有数据导入失败
        </strong>
    </div>
}
else
{
    <div class="row">
        <div class="col-xs-12 col-sm-12 widget-container-span ui-sortable">
            <div class="widget-box">
                <div class="widget-header header-color-green">
                    <h5>导入成功的数据</h5>

                    <div class="widget-toolbar">
                        <a href="#" data-action="collapse">
                            <i class="1 bigger-125 icon-chevron-up"></i>
                        </a>
                    </div>

                    <div class="widget-toolbar no-border">
                        <a class="btn btn-xs btn-success bigger" target="_blank" href="@Url.Action("Export", "LngAllowance", new { exportCode = ViewBag.SuccessCode.ToString() })">
                            <i class="icon-share"></i>
                            导出
                        </a>
                    </div>
                </div>

                <div class="widget-body">
                    <div class="widget-body-inner" style="display: block;">
                        <div class="widget-main">

                            <div class="table-responsive">
                                <table id="sample-table-2" class="table table-striped table-bordered table-hover">
                                    <thead>
                                    <tr>
                                        <th class="center">
                                            <label>
                                                <input type="checkbox" class="ace" />
                                                <span class="lbl"></span>
                                            </label>
                                        </th>
                                        <th>编号</th>
                                        <th>企业名称</th>
                                        <th>批次</th>
                                        <th>车牌号</th>
                                        <th>发动机号</th>
                                        <th>气瓶1编号</th>
                                        <th>气瓶2编号</th>
                                    </tr>
                                    </thead>
                                    <tbody>

                                    @foreach (var item in successList)
                                    {
                                        <tr>
                                            <td class="center">
                                                <label>
                                                    <input type="checkbox" class="ace" />
                                                    <span class="lbl"></span>
                                                </label>
                                            </td>
                                            <td>
                                                <a href="javascript:;">@item.Id</a>
                                            </td>
                                            <td>
                                                @item.CompanyName
                                            </td>
                                            <td>
                                                @item.LotId
                                            </td>
                                            <td>
                                                @item.TruckNo
                                            </td>
                                            <td>@item.EngineId</td>
                                            <td>
                                                @item.CylinderDefaultId.Replace('_', '-')
                                            </td>
                                            <td>
                                                @(item.CylinderSeconedId.IsNullOrEmpty() ? "无" : item.CylinderSeconedId.Replace('_', '-'))
                                            </td>
                                        </tr>
                                    }
                                    </tbody>
                                </table>
                            </div>



                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

