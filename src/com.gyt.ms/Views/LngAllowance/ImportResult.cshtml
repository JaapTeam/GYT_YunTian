@using Castle.Core.Internal
@using Zer.GytDto
@{
    var successList = ViewBag.SuccessList as List<LngAllowanceInfoDto>;
    var failedList = ViewBag.FailedList as List<LngAllowanceInfoDto>;
    var existedList = ViewBag.ExistedList as List<LngAllowanceInfoDto>;
}

<!--导入失败的列表-->

@if (!failedList.IsNullOrEmpty())
{
    <div class="table-header">
        <div class="visible-sm visible-md hidden-sm hidden-xs action-buttons">
            <a class="white" target="_blank" href="@(Url.Action("Export", "LngAllowance", new { exportList = failedList }))">
                <i class="icon-cloud-download bigger-110">
                    <span class="">导出</span>
                </i>
            </a>
        </div>
    </div>
    <div class="table-responsive">
        <table id="sample-table-2" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th class="center">
                        <label>
                            <input type="checkbox" class="ace" />
                            <span class="lbl"></span>
                        </label>
                    </th>
                    <th>编号</th>
                    <th>企业名称</th>
                    <th>批次</th>
                    <th>车牌号</th>
                    <th>发动机号</th>
                    <th>气瓶1编号</th>
                    <th>气瓶2编号</th>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in failedList)
                {
                    <tr>
                        <td class="center">
                            <label>
                                <input type="checkbox" class="ace" />
                                <span class="lbl"></span>
                            </label>
                        </td>
                        <td>
                            <a href="javascript:;">@item.Id</a>
                        </td>
                        <td>
                            @item.CompanyName
                        </td>
                        <td>
                            @item.LotId
                        </td>
                        <td>
                            @item.TruckNo
                        </td>
                        <td>@item.EngineId</td>
                        <td>
                            @item.CylinderDefaultId.Replace('_', '-')
                        </td>
                        <td>
                            @(item.CylinderSeconedId.IsNullOrEmpty() ? "无" : item.CylinderSeconedId.Replace('_', '-'))
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}

<!--重复数据的列表-->


